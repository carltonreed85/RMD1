# Initialize git repository
git init

# Create .gitignore file
echo "node_modules/
dist/
.env
logs/
*.log" > .gitignore

# Add all files
git add .

# Create initial commit
git commit -m "Initial commit: Healthcare Referral Management System"

# Add the GitHub repository as remote
# Replace YOUR_USERNAME with your GitHub username
git remote add origin https://github.com/YOUR_USERNAME/rmd-app.git

# Push code to GitHub
git push -u origin main
